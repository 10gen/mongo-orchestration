{
    "id" : "standalone",
    "name": "mongod",
    "procParams": {
        "dbpath": "$DBPATH", 
        "ipv6": true, 
        "logappend": true, 
        "logpath": "$LOGPATH/mongod.log", 
        "journal": true,
        "port": 27017
    }, 
    "sslParams": {
        "sslCAFile": "$SSL_FILES/ca.pem", 
        "sslOnNormalPorts": true, 
        "sslPEMKeyFile": "$SSL_FILES/server.pem", 
        "sslWeakCertificateValidation": true
    }
}
